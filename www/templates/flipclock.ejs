		<% for(var s=0; s<scores.length; s++) { %>
			<div class="<%= scores[s]%> score">
			
			    <ul class="flip minutePlay">
				<% for(var i=0; i<numbers[0]; i++) { %>
			        <li>
			            <a href="#">
			                <div class="up">
			                    <div class="shadow"></div>
			                    <div class="inn"><%= i %></div>
			                </div>
			                <div class="down">
			                    <div class="shadow"></div>
			                    <div class="inn"><%= i %></div>
			                </div>
			            </a>
			        </li>
				<% } %>
			    </ul>
			    <ul class="flip secondPlay">
				<% for(var i=0; i<numbers[1]; i++) { %>
			        <li>
			            <a href="#">
			                <div class="up">
			                    <div class="shadow"></div>
			                    <div class="inn"><%= i %></div>
			                </div>
			                <div class="down">
			                    <div class="shadow"></div>
			                    <div class="inn"><%= i %></div>
			                </div>
			            </a>
			        </li>
				<% } %>
				</ul>

			</div>		
		<% } %>
		